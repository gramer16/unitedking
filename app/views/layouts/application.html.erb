<!DOCTYPE html>
<html>
<head>
  <title>UnitedKingdomOnlyRealEstate</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
   <%= javascript_include_tag "https://js.stripe.com/v2/", 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= tag :meta, :name => "stripe-key", :content => STRIPE_PUBLIC_KEY %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href=”css/bootstrap.min.css” rel=”stylesheet” media=”screen”>
  <link href="assets/css/bootstrap-responsive.css" rel="stylesheet">
  
</head>
<body>
	
	<%= render 'layouts/header' %>
  
	<div class="container">
      <% flash.each do |name, msg| %>
    <% if msg.is_a?(String) %>
      <div class="alert alert-<%= name == :notice ? "success" : "success" %> alert-dismissable">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
        <%= content_tag :div, msg, :id => "flash_#{name}" %>
      </div>
    <% end %>
  <% end %>
		<%= yield %>
    <script src="jquery.js"></script>
    <script src="typeahead.js"></script>
    <script>
    $("#query").typeahead({
      name: "residential",
      remote: "/residentials/autocomplete?query=%QUERY"
    });
    </script>
    <script src="http://code.jquery.com/jquery.js"></script>
    <script src="js/bootstrap.min.js"></script>

	<%= render 'layouts/footer' %>
</div>

</body>
</html>
